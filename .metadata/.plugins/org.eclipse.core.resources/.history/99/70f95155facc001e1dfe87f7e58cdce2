package Exemplos;

import java.util.Scanner;

public class ExemploTryCatch {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		int idade = 0;
		
					
		
		do {
			System.out.println("Qual sua idade ?");	
			try {
				idade = sc.nextInt();
			} catch (Exception e) {
				System.out.println("VocÃª digitou errado!");
				sc.nextLine();
			}
		} while (idade <= 0 );
		
	
		
		
		
				
		
	
		
		
		
		
	}
}
